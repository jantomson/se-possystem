<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
    <VBox alignment="CENTER" spacing="10">
        <Label fx:id="teamNameLabel" />
        <Label fx:id="contactLabel" />
        <Label fx:id="membersLabel" />
        <ImageView fx:id="teamLogoImage" fitWidth="200" fitHeight="200" preserveRatio="true" />
    </VBox>
</AnchorPane>